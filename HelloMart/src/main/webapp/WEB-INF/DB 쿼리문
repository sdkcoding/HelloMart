/* 
- 유저 생성
 create user jspid@localhost identified by 'jsppass';
 grant all privileges on *.* to jspid@localhost identified by 'jsppass' with grant option;
 
- 데이터 베이스 생성 구문
 DROP DATABASE IF EXISTS Hellomart;
 CREATE DATABASE HelloMart;
 USE HelloMart;
 */

DROP TABLE IF EXISTS Account;

CREATE TABLE Account (
ID 		 CHAR(20),
Password	 	 CHAR(16) NOT NULL,
Role		 ENUM('MEMBER', 'SELLER', 'ADMIN') DEFAULT 'MEMBER'  NOT NULL,
Points		 INT DEFAULT 0 NOT NULL, 
Grade		 TINYINT DEFAULT 0 NOT NULL,  /* 등급 숫자로 포인트 적립 가중치 */
Email		 VARCHAR(100) NOT NULL,
PostCode		 CHAR(5) NOT NULL,
RoadAddress	 VARCHAR(50) NOT NULL,
DetailAddress	 VARCHAR(50) NOT NULL,
Phone		 CHAR(11) NOT NULL,
Name		 CHAR(20) NOT NULL,
BirthYear		 CHAR(4) NOT NULL,
BirthMonth	 CHAR(2) NOT NULL,
BirthDay		 CHAR(2) NOT NULL,
Gender		 ENUM('F','M') NOT NULL,
Reliability		 INT DEFAULT NULL,   /* 판매자만 신뢰도 가짐 */
JoinDate		 TIMESTAMP DEFAULT CURRENT_TIMESTAMP NOT NULL,
Apply		 ENUM('SELLER_READY', 'NOTHING') DEFAULT 'NOTHING' NOT NULL
);

ALTER TABLE Account ADD CONSTRAINT acc_id_pk PRIMARY KEY(ID);
ALTER TABLE Account ADD CONSTRAINT acc_email_uk UNIQUE(Email);

