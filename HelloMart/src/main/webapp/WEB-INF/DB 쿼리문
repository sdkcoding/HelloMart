DROP TABLE IF EXISTS ProductList;

CREATE TABLE ProductList (
No		 INT AUTO_INCREMENT PRIMARY KEY, 
ProductName	 VARCHAR(50) NOT NULL,
MainCategory	 CHAR(20)  NOT NULL,
SmallCategory 	 CHAR(20) NOT NULL,
ImagePath	 VARCHAR(256) NOT NULL,
OrderCount	 INT DEFAULT 0 NOT NULL,
Score		 INT DEFAULT 0 NOT NULL,
ProdDate		 Date NOT NULL,	
MfCompany	 CHAR(30) NOT NULL,
Price		 INT NOT NULL,
Weight		 DECIMAL(9, 2) NOT NULL,
RegisterID	 CHAR(32) NOT NULL,
Comment		 VARCHAR(100),
ReviewCount	  INT DEFAULT 0 NOT NULL,
RegisterDate	 TIMESTAMP DEFAULT CURRENT_TIMESTAMP NOT NULL
);

ALTER TABLE ProductList
ADD CONSTRAINT prolist_rid_fk FOREIGN KEY(RegisterID) REFERENCES Account (ID) ON DELETE CASCADE;


/*냉장고*/
DROP TABLE IF EXISTS Refrigerator;

CREATE TABLE Refrigerator(
No		 	INT NOT NULL,
ColdStorageCapacity 	INT,				/*냉장용량*/
FreezeCapacity 	 	INT,				/*냉동용량*/
DoorNumber 	 	ENUM('일반형냉장고', '양문형냉장고', '3도어냉장고', '4도어냉장고'), /*도어수*/
PowerConsumption 	DECIMAL(3, 1),		/*소비전력*/
EnergyEfficiency 		ENUM('1등급', '2등급', '3등급', '4등급', '5등급')		/*에너지효율*/
);

ALTER TABLE Refrigerator
ADD CONSTRAINT refrigerator_no_fk FOREIGN KEY(No) REFERENCES ProductList (No) ON DELETE CASCADE;

/*전자레인지_오븐*/
DROP TABLE IF EXISTS Microwave;

CREATE TABLE Microwave(
No 			INT NOT NULL,				
Capacity 			INT,				/*용량*/
ControlMethod 		ENUM('버튼식', '다이얼식', '버튼+다이얼식'), /*조작방식*/
PowerConsumption 	INT,				/*소비전력*/
Watt			 INT			/*출력*/
);

ALTER TABLE Microwave
ADD CONSTRAINT microwave_no_fk FOREIGN KEY(No) REFERENCES ProductList (No) ON DELETE CASCADE;


DROP TABLE IF EXISTS Cleaner;

/* 청소기 */
CREATE TABLE Cleaner(
No 		 INT NOT NULL,
WireLess 		 ENUM('유선','무선') NOT NULL,
ChargingTime 	 INT,
CONSTRAINT cleaner_no_fk FOREIGN KEY(No) REFERENCES ProductList(No) ON DELETE CASCADE
);


DROP TABLE IF EXISTS Washer;

/* 세탁기 */
CREATE TABLE Washer(
No INT NOT NULL,
WashingCapacity INT NOT NULL,
EnergyEfficiency enum('1등급','2등급','3등급','4등급','5등급')  NOT NULL,  
WashingType ENUM('통돌이', '세탁전용', '공기방울') NOT NULL,  
CONSTRAINT washer_no_fk FOREIGN KEY(No) REFERENCES ProductList(No) ON DELETE CASCADE
);

DROP TABLE IF EXISTS AirConditioner;

/* 에어컨 */
CREATE TABLE AirConditioner(
No INT NOT NULL,
SquareMeasure DECIMAL(3, 1) NOT NULL,  /* 사용면적 */
Cooling DECIMAL(2, 1) NOT NULL,  /* 냉방 효율 */
Dehumidification ENUM('가능', '불가능') NOT NULL,  /* 제습운전가능여부 */
AirPurification ENUM('가능','불가능') NOT NULL, /* 공기청정기능여부 */
LowNoise ENUM('가능','불가능') NOT NULL, /* 저소음운전가능여부 */
Type ENUM('복합형', '스탠드형', '벽걸이형') NOT NULL,
CONSTRAINT airconditioner_no_fk FOREIGN KEY(No) REFERENCES ProductList(No) ON DELETE CASCADE
);

/*공기청정기_가습기*/
DROP TABLE IF EXISTS AirPurifier;

CREATE TABLE AirPurifier(
No 			INT NOT NULL,
Method 			ENUM('필터식청정','복합식청정','음이온식청정','산림욕식청정'),        /*공기청정방식*/
UsableArea 		INT,			/*사용면적*/
FilterType 		ENUM('필터','헤파필터'),	/*필터종류*/
PowerConsumption 	INT	/*소비전력*/
);

ALTER TABLE AirPurifier
ADD CONSTRAINT airpurifier_no_fk FOREIGN KEY(No) REFERENCES ProductList (No) ON DELETE CASCADE;

DROP TABLE IF EXISTS Monitor;

/* 모니터 */
CREATE TABLE Monitor(
No INT NOT NULL,
ScreenSize DECIMAL(4, 1) NOT NULL,
Ratio ENUM('21:9', '16:10', '16:9', '4:3') NOT NULL,
Resolution CHAR(9) NOT NULL,
CONSTRAINT monitor_no_fk FOREIGN KEY(No) REFERENCES ProductList(No) ON DELETE CASCADE
);

DROP TABLE IF EXISTS Printer;

/*프린터*/
CREATE TABLE Printer(
No INT NOT NULL,
Method ENUM('잉크젯', '레이저'),
Type ENUM('흑백','컬러'),
PPM INT NOT NULL,  /* 인쇄 속도 */
Resolution ENUM('4800x1200','5760x1440','1200x1200','2400x600','9600x600'),
Scan ENUM('가능','불가능'),  /* 스캔 기능 유무 */
Duplication ENUM('가능','불가능'),  /* 복사 기능 유무 */
Fax ENUM('가능','불가능') /* 팩스 기능 유무 */
);
ALTER TABLE Printer Add CONSTRAINT prin_no_fk FOREIGN KEY(No) REFERENCES ProductList(NO) ON DELETE CASCADE;

/*데스크탑*/
DROP TABLE IF EXISTS Desktop;

CREATE TABLE Desktop(
No INT NOT NULL,
Shape ENUM('AllInOne', 'WorkStation', 'BareBones') NOT NULL,   /*데스크탑 형태*/
CpuMfCompany ENUM('Intel', 'AMD') NOT NULL,
CpuKind enum('셀러론', 'I3', 'I5', 'I7', '라이젠3', '라이젠5', '라이젠7') NOT NULL,
CpuCore ENUM('2(듀얼)', '4(쿼드)', '6(헥사)', '8(옥타)') NOT NULL,
StoreageKind ENUM('SSD', 'HDD') NOT NULL,
HardCap SMALLINT NOT NULL,
ScreenSize DECIMAL(3, 1) NOT NULL,
Resolution CHAR(9) NOT NULL
);

ALTER TABLE Desktop
ADD CONSTRAINT desktop_no_fk FOREIGN KEY(No) REFERENCES ProductList(No) ON DELETE CASCADE;

/*노트북*/
DROP TABLE IF EXISTS Notebook;

CREATE TABLE Notebook (
No INT NOT NULL,
CpuMfCompany ENUM('Intel', 'AMD') NOT NULL,
CpuKind enum('셀러론', 'I3', 'I5', 'I7', '라이젠3', '라이젠5', '라이젠7') NOT NULL,
CpuCore ENUM('2(듀얼)', '4(쿼드)', '6(헥사)', '8(옥타)') NOT NULL,
StoreageKind ENUM('SSD', 'HDD') NOT NULL,
HardCap SMALLINT NOT NULL,
ScreenSize DECIMAL(3, 1) NOT NULL,
Resolution CHAR(9) NOT NULL
);

ALTER TABLE Notebook
ADD CONSTRAINT notebook_no_fk FOREIGN KEY(No) REFERENCES ProductList(No) ON DELETE CASCADE;


DROP TABLE IF EXISTS Smartphone;

/*스마트*/
CREATE TABLE Smartphone(
No		 INT NOT NULL,
Model		 enum('갤럭시S', '갤럭시노트', '갤럭시A/J', '아이폰', 'LG 시리즈') NOT NULL,
OS		 ENUM('Android','iOS'),
Battery 		 INT NOT NULL,
LCT		 ENUM('Amoled','LCD'),
SS 		 DECIMAL(5, 2) NOT NULL,  /* 화면크기 */
CONSTRAINT smat_no_fk FOREIGN KEY(No) REFERENCES ProductList(No) ON DELETE CASCADE
);

DROP TABLE IF EXISTS Tablet;

/*태블릿*/
CREATE TABLE Tablet(
No		 INT NOT NULL,
Model		 enum('APPLE', '삼성전자', '레노버', 'LG전자',  'Microsoft') NOT NULL,
OS		 ENUM('Android','iOS','Window'),
Battery		 INT NOT NULL,
LCT		 ENUM('Amoled','LCD'),
SS 		 DECIMAL(5, 2) NOT NULL,  /* 화면크기 */
CONSTRAINT tab_tno_fk FOREIGN KEY(No) REFERENCES ProductList(No) ON DELETE CASCADE
);



DROP TABLE IF EXISTS orderList;

/* 주문 */
CREATE TABLE OrderList(
Idx			 INT AUTO_INCREMENT PRIMARY KEY, 
OrderId			 CHAR(32) NOT NULL,
ReceiverName		 CHAR(5) NOT NULL,
ReceiverPhone		 CHAR(15) NOT NULL,
ProdNo 			 INT NOT NULL,
OrderCount		 INT NOT NULL,
OrderDate 		 TIMESTAMP DEFAULT CURRENT_TIMESTAMP NOT NULL,
OrderPrice		 int NOT NULL,    /* 옵션(?) + 원가 x 수량 */
ReceiverPostCode	 CHAR(5) NOT NULL,
ReceiverRoadAddress	 VARCHAR(50) NOT NULL,
ReceiverDetailAddress	 VARCHAR(50) NOT NULL,
OrderStatus 		 ENUM('PAY_WAIT', 'PAY_OK', 'SEND_WAIT', 'SEND_OK') not null,
CONSTRAINT ord_id_fk FOREIGN KEY(OrderId) REFERENCES Account(ID) ON DELETE CASCADE,
CONSTRAINT ord_prodno_fk FOREIGN KEY(ProdNo ) REFERENCES ProductList(No) ON DELETE CASCADE
);



DROP TABLE IF EXISTS CartList;

/* 장바구니 */
CREATE TABLE CartList (
Idx 		INT AUTO_INCREMENT PRIMARY KEY,
Id 			CHAR(20) NOT NULL,
No 		INT NOT NULL,
Date 		TIMESTAMP DEFAULT CURRENT_TIMESTAMP NOT NULL,
OrderCount 		INT NOT NULL,
CONSTRAINT cl_id_fk FOREIGN KEY (Id) REFERENCES Account (Id) ON DELETE CASCADE,
CONSTRAINT cl_no_fk FOREIGN KEY (No) REFERENCES ProductList (No) ON DELETE CASCADE
);

DROP TABLE IF EXISTS Review;

/* 리뷰 */
CREATE TABLE Review (
  Idx INT NOT NULL AUTO_INCREMENT PRIMARY KEY,     /*리뷰 번호*/
  No INT NOT NULL, /*리뷰 상품번호*/
  Content VARCHAR(200) NOT NULL,              /*리뷰 내용*/
  Id VARCHAR(50) NOT NULL,                /*리뷰 작성자*/
  Star INT NOT NULL, /* 별 */
  Regdate TIMESTAMP NOT NULL DEFAULT NOW(),   /*리뷰 등록시간*/
  CONSTRAINT review_no_fk FOREIGN KEY (No) REFERENCES ProductList (No) ON DELETE CASCADE
);

DROP TABLE IF EXISTS QABoard;

/* Q&A 게시판 */
CREATE TABLE QABoard (
Idx		 INT AUTO_INCREMENT PRIMARY KEY,
Subject 		 VARCHAR(50) NOT NULL,
Content 	 	 VARCHAR(200) NOT NULL,
Id 			 CHAR(20) NOT NULL,
Count 	 	  int not null default 0,
Date	  	 TIMESTAMP DEFAULT CURRENT_TIMESTAMP NOT NULL,
moDate		 datetime default null,
cmt		   int not null default 0,  /* 코멘트에대한 카운트  */
constraint qa_id_fk foreign key(id) references account(id) on delete cascade
);

	/* Q&A 게시판 더미*/
