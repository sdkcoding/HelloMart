/* 코멘트 게시판 */
create table cmtboard(
	cmtIdx int auto_increment primary key,
    Id char(20) not null,
    date timestamp default current_timestamp not null,
    cmtPar int not null,
    content varchar(200) not null,
    constraint cb_cp_fk foreign key(cmtPar) references qaboard(idx) on delete cascade
);


/* 포인트 이력 */
CREATE TABLE Point(
Idx 		 INT AUTO_INCREMENT PRIMARY KEY,
Id 			 CHAR(20) NOT NULL,
IncDec		 ENUM('+','-') NOT NULL,    /*포인트 증감*/
Point		 INT NOT NULL,	/*변동 포인트*/
Date 		 TIMESTAMP DEFAULT CURRENT_TIMESTAMP NOT NULL,
Content 		 VARCHAR(200) NOT NULL,  /*변동 내역*/
CONSTRAINT p_id_fk FOREIGN KEY (Id ) REFERENCES Account(Id ) ON DELETE CASCADE
);





